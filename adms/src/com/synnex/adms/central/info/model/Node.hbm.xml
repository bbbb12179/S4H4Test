<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
      
<hibernate-mapping>  
	<class name="com.synnex.adms.central.model.Node" table="NODE" dynamic-insert="true" dynamic-update="true">  
  
  		<id name="nodeId" column="NODE_ID"/>
        <property name="nodeQueueName" column="NODE_QUEUENAME"/>
		<property name="nodeIp" column="NODE_IP"/>
		<property name="nodePort" column="NODE_PORT"/>
	</class>  
</hibernate-mapping>